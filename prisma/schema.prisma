model users {
  name  String
  id    String @id
  orgId String
  email String
}

model original_components {
  id                 String  @id
  orgId              String
  computer_name      String
  os                 String
  processor_name     String
  storage_capacity   String
  storage_drives     String
  ram                String
  ram_slots          String
  graphics_card      String
  vram               String
  peripheral_devices String
  date_of_addition   String
}

model updated_components {
  id                  String @id
  orgId               String
  computer_name       String
  os                  String
  storage_capacity    String
  storage_drives      String
  processor_name      String
  ram                 String
  ram_slots           String
  graphics_card       String
  vram                String
  peripheral_devices  String
  last_date_of_update String
}

model update_log {
  log_id                 String @id
  id                     String
  orgId                  String
  old_computer_name      String
  new_computer_name      String
  old_os                 String
  new_os                 String
  old_processor_name     String
  new_processor_name     String
  old_storage_capacity   String
  new_storage_capacity   String
  old_storage_drives     String
  new_storage_drives     String
  old_ram                String
  new_ram                String
  old_ram_slots          String
  new_ram_slots          String
  old_graphics_card      String
  new_graphics_card      String
  old_vram               String
  new_vram               String
  old_peripheral_devices String
  new_peripheral_devices String
  last_date_of_update    String
}
